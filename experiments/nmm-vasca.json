{
  "project": "isnmm",
  "experiment": "nnmm-vasca",
  "model": "VASCA",
  "dataset": "SLS",
  "train":
  {
    "log": "parameters",
    "accelerator": "cpu",
    "epochs": 10,
    "metrics": ["h_R-squared", "A_MSE_dB", "A_SAN", "z_subspace", "z_MSE_dB"],
    "monitor": "A_MSE_dB",
    "patience": 3,
    "tolerance": 1e-2,
    "batch_size": 100,
    "lr": {
      "th": 0.001,
      "ph": 0.01
    },
    "seed": null,
    "mc_samples": 100
  },
  "data": {
    "model": "polynomial",
    "degree": 10,
    "latent_dim": 3,
    "observed_dim": 4,
    "size": 100,
    "SNR": 25
  },
  "encoder": {
    "hidden_dim": {
      "h1":  64,
      "h2":  32,
      "h3":  16,
      "h4":  8
    }
  },
  "decoder": {
    "hidden_dim": {
      "h1":  128,
      "h2":  64,
      "h3":  64,
      "h4":  32
    },
    "activation": "Sigmoid"
  }
}