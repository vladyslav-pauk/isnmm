{
  "model_name": "cnae",
  "torch_seed": 42,
  "data_loader": {
    "module_name": "synthetic",
    "batch_size": 1000,
    "split": [0.8, 0.1, 0.1],
    "num_workers": 4
  },
  "model": {
      "mc_samples": 100
    },
  "train": {
    "lr": {
      "encoder": 0.001,
      "decoder": 0.001
    },
    "rho": 100.0,
    "inner_iters": 100,
    "log_monitor": {
      "monitor": "train_loss",
      "mode": "min"
    }
  },
  "optimizer": {
    "name": "Adam",
    "params": {
    },
    "lr": {
      "th": {
        "l": 0.01,
        "nl": 0.001
      },
      "ph": 0.01
    }
  },
  "trainer": {
    "accelerator": "auto",
    "devices": 1,
    "max_epochs": 200,
    "log_every_n_steps": 10,
    "enable_progress_bar": false
  },
  "checkpoint": {
    "monitor": "train_loss",
    "mode": "min",
    "save_top_k": 1,
    "verbose": true
  },
  "early_stopping": {
    "monitor": "train_loss",
    "mode": "min",
    "patience": 50,
    "min_delta": 1e-3
  },
  "encoder": {
    "hidden_layers": [128, 128, 128],
    "groups": null
  },
  "decoder": {
    "hidden_layers": [128, 128, 128],
    "groups": null
  }
}