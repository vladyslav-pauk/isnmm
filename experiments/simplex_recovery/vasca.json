{
  "model_name": "vasca",
  "torch_seed": null,
  "data_loader": {
    "module_name": "synthetic",
    "batch_size": 100,
    "split": [0.8, 0.1, 0.1],
    "num_workers": 4
  },
  "model": {
    "mc_samples": 1
  },
  "trainer": {
    "accelerator": "cpu",
    "max_epochs": 500,
    "log_every_n_steps": 4,
    "limit_val_batches": 1.0,
    "val_check_interval": 1.0,
    "limit_test_batches": 1,
    "gradient_clip_val": 0.5,
    "enable_progress_bar": false
  },
  "encoder": {
    "hidden_layers": {
      "h1":  32,
      "h2":  32,
      "h3":  16,
      "h4":  8
    },
    "hidden_activation": "relu",
    "output_activation": null,
    "weight_initialization": null,
    "bn_eps": 1e-5,
    "bn_momentum": 0.1,
    "dropout_rate": null
  },
  "decoder": {
      "mixture_initialization": null
  },
  "optimizer": {
    "name": "Adam",
    "params": {
    },
    "lr": {
      "th": 0.01,
      "ph": 0.01
    }
  },
  "checkpoint": {
    "monitor": "mixture_mse_db",
    "mode": "min",
    "save_top_k": 1,
    "verbose": true
  },
  "early_stopping": {
    "monitor": "mixture_log_volume",
    "mode": "max",
    "patience": 10,
    "min_delta": 1e-3
  },
  "schedule": {
    "repeats": 100,
    "parameters": {
      "train":
      {
        "lr": {
          "th": [0.001, 0.01],
          "ph": [0.01, 0.005]
        }
      },
      "data": {
        "SNR": [10, 20, 30],
        "seed": [29, 0, 42, 1, 2]
      }
    }
  }
}