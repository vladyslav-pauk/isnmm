{
  "model_name": "vasca",
  "torch_seed": 0,
  "metric": {
    "goal": "minimize",
    "name": "latent_mse"
  },
  "data_loader": {
    "module_name": "synthetic",
    "split": [1.0, 0.1, 0.1],
    "batch_size": 100,
    "val_batch_size": 100,
    "num_workers": 4,
    "shuffle": true
  },
  "model": {
    "latent_dim": null,
    "mc_samples": 1,
    "sigma": null,
    "distance": "mse_loss",
    "reparameterization": "LogitTransform",
    "unmixing": null,
    "prior": {
      "base_distribution": "Dirichlet"
    },
    "posterior": {
      "base_distribution": "MultivariateNormal"
    }
  },
  "encoder": {
    "constructor": "FCN",
    "hidden_layers": {
      "h1": 32,
      "h2": 32,
      "h3": 16,
      "h4": 8
    },
    "hidden_activation": "ReLU",
    "output_activation": null,
    "weight_initialization": null,
    "bn_eps": 1e-5,
    "bn_momentum": 0.1,
    "dropout_rate": null
  },
  "decoder": {
    "mixture_initialization": null
  },
  "optimizer": {
    "name": "Adam",
    "params": {
    },
    "lr": {
      "decoder": 0.001,
      "encoder": 0.01
    },
    "scheduler": null,
      "scheduler_params": {
        "interval": "epoch",
        "frequency": 1,
        "monitor": "latent_mse",
        "strict": true
      }
  },
  "trainer": {
    "accelerator": "cpu",
    "devices": 1,
    "max_epochs": 20000,
    "log_every_n_steps": null,
    "limit_val_batches": 1,
    "val_check_interval": null,
    "limit_test_batches": 1,
    "gradient_clip_val": null,
    "check_val_every_n_epoch": 10,
    "enable_progress_bar": false
  },
  "checkpoint": {
    "save_top_k": 1,
    "verbose": true
  },
  "early_stopping": {
    "patience": 100,
    "min_delta": 1e-4
  }
}